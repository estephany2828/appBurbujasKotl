<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Formulario" tools:layout_editor_absoluteY="25dp">

    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:id="@+id/textView7"
            tools:ignore="MissingConstraints" android:layout_marginTop="70dp"
            app:layout_constraintEnd_toStartOf="@+id/editText6" android:layout_marginBottom="69dp"
            app:layout_constraintBottom_toTopOf="@+id/textView8" app:layout_constraintTop_toTopOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="3dp"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editText6"
            tools:ignore="MissingConstraints" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="29dp"
    />
    <TextView
            android:text="Telefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/editText7" android:layout_marginStart="29dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editText7"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/edDirFor" android:layout_marginBottom="23dp"
            app:layout_constraintBottom_toTopOf="@+id/edDirFor"/>
    <TextView
            android:text="Direccion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            tools:ignore="MissingConstraints" android:layout_marginTop="10dp"
            app:layout_constraintEnd_toStartOf="@+id/edDirFor" app:layout_constraintTop_toTopOf="@+id/edDirFor"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="29dp" android:layout_marginEnd="24dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/edDirFor"
            tools:ignore="MissingConstraints"
            app:layout_constraintBaseline_toBaselineOf="@+id/btnMapa"
            app:layout_constraintStart_toEndOf="@+id/textView10" app:layout_constraintEnd_toStartOf="@+id/btnMapa"
            android:layout_marginEnd="14dp"/>
    <Button
            android:text="B"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:id="@+id/btnMapa"
            android:background="@color/colorAccent"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="231dp"
            app:layout_constraintStart_toEndOf="@+id/edDirFor" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Informacion Personal"
            android:layout_width="0dp"
            android:layout_height="34dp"
            android:id="@+id/textView11"
            android:textSize="18sp" android:textAlignment="center"
            android:textColor="@color/colorAccent"
            tools:ignore="MissingConstraints" android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="50dp" android:layout_marginEnd="50dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnSig" tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/edDirFor"
            android:layout_marginTop="69dp" android:layout_marginStart="37dp"
            app:layout_constraintTop_toBottomOf="@+id/edDirFor"/>
</android.support.constraint.ConstraintLayout>